    @{
        Layout = null;
    }
    <h2>People</h2>
    <p>
        <a href="#/Person/Create">Create New</a>
    </p>
    <table class="table">
        <thead>
        <tr>
            <th>
                First Name
            </th>
            <th>
                Last Name
            </th>
            <th>
                DOB
            </th>
            <th>
                Address
            </th>
            <th>
                Address 2
            </th>
            <th>
                City
            </th>
            <th>
                Suburb
            </th>
            <th>
                Postcode
            </th>
            <th></th>
        </tr>
        </thead>
        <tbody>
        <tr ng-repeat="item in data">
            <td>
                {{ item.FName }}
            </td>
            <td>
                {{ item.LName }}
            </td>
            <td>
                {{ item.Dob | date }}
            </td>
            <td>
                {{ item.Address }}
            </td>
            <td>
                {{ item.Address2 }}
            </td>
            <td>
                {{ item.City }}
            </td>
            <td>
                {{ item.Suburb }}
            </td>
            <td>
                {{ item.Postcode }}
            </td>
            <td>
                <a ng-href="#/Person/Edit/{{item.Id}}">Edit</a> |
                <a ng-href="#/Person/{{item.Id}}">Details</a> |
                <a ng-href="#/Person/Delete/{{item.Id}}">Delete</a>
            </td>
        </tr>
        </tbody>
    
    </table>
